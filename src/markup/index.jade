// ------------------ mixins

mixin taglist(stack)
    ul.tags 
        each val in stack
            li=val

mixin imglist(projects, company)
    ul.imageList
        each val in projects 
            if val 
                li 
                    figure(class="all-#{company} s-#{val.id}")
                    h3 #{val.name}
                    if val.link
                        a(href="#{val.link}", target="_blank", class="cc-eye") visit 
                    if val.detail
                        a(href="#{val.detail}.html", class="cc-pictures", target="_blank") detail 
                    if val.gitsample 
                        a(href="#{val.gitsample}", class="cc-github", target="_blank") code  
                    if val.stack 
                        +taglist(val.stack)
                    


